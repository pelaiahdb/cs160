<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.monsgoblin.hifi_prototype.MapsActivity">

    <ImageView
        android:id="@+id/compass"
        android:src="@drawable/compasswheelmenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:layout_marginTop="20dp"
        android:layout_marginRight ="20dp"/>

    <ImageView
        android:id="@+id/button1"
        android:src="@drawable/vistoall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:layout_marginTop="5dp"
        android:layout_marginRight ="105dp"
        android:alpha="0.7"/>

    <ImageView
        android:id="@+id/button2"
        android:src="@drawable/allgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:layout_marginTop="110dp"
        android:layout_marginRight ="110dp"
        android:alpha="0.7"/>

    <ImageView
        android:id="@+id/button3"
        android:src="@drawable/showall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|top"
        android:layout_marginTop="110dp"
        android:layout_marginRight ="5dp"
        android:alpha="0.7"/>

    <ImageView
        android:id="@+id/button4"
        android:src="@drawable/reqgroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="240dp"
        android:layout_marginLeft ="10dp"
        android:alpha="0.7"/>

    <ImageView
        android:id="@+id/button5"
        android:src="@drawable/schedgroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="135dp"
        android:layout_marginLeft ="10dp"
        android:alpha="0.7"/>

    <ImageView
        android:id="@+id/button6"
        android:src="@drawable/repalert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft ="10dp"
        android:alpha="0.7"/>

    <ImageView
        android:id="@+id/button7"
        android:src="@drawable/watchover"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft ="115dp"
        android:alpha="0.7"/>


    <com.skyfishjy.library.RippleBackground
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/content1"
        app:rb_color="#00FFFF"
        app:rb_radius="200dp"
        app:rb_rippleAmount="1"
        app:rb_duration="3000"
        app:rb_scale="6"
        app:rb_type="fillRipple"
        android:alpha="0.7">
    </com.skyfishjy.library.RippleBackground>

    <com.skyfishjy.library.RippleBackground
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/content2"
        app:rb_color="#FF69B4"
        app:rb_radius="200dp"
        app:rb_rippleAmount="4"
        app:rb_duration="3000"
        app:rb_scale="6"
        app:rb_type="strokeRipple"
        android:alpha="0.7">
    </com.skyfishjy.library.RippleBackground>

    <com.skyfishjy.library.RippleBackground
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/content3"
        app:rb_color="#FFCC00"
        app:rb_radius="50dp"
        app:rb_rippleAmount="3"
        app:rb_duration="3000"
        app:rb_scale="4"
        app:rb_type="fillRipple"
        android:alpha="0.7">
    </com.skyfishjy.library.RippleBackground>

    <com.skyfishjy.library.RippleBackground
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/content4"
        app:rb_color="#66FF00"
        app:rb_radius="50dp"
        app:rb_rippleAmount="7"
        app:rb_duration="3000"
        app:rb_scale="5"
        app:rb_type="strokeRipple"
        android:alpha="0.7">
    </com.skyfishjy.library.RippleBackground>

</fragment>